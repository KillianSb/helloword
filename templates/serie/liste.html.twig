{% extends 'base.html.twig' %}

{% block title %}Hello SerieController!{% endblock %}

{% block body %}
    <h1>Toutes les series</h1>
    {% for serie in series %}
        <div>
            <a href="{{ path('serie_detail', {'serie': serie.id }) }}">
                <div>{{ serie.titre }}</div>
            </a>
        </div>
    {% endfor %}
{% endblock %}
